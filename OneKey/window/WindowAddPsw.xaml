        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="创建密码" Height="137" Width="300" ShowInTaskbar="False" ResizeMode="NoResize" WindowStartupLocation="CenterOwner" Icon="/OneKey;component/resources/main.ico" Closed="Window_Closed">
    <Grid>
        <PasswordBox x:Name="PswBoxFirst" Margin="87,10,10,0" VerticalAlignment="Top" Height="25" VerticalContentAlignment="Center"/>
        <PasswordBox x:Name="PswBoxSecond" Margin="87,40,10,0" VerticalAlignment="Top" Height="25" VerticalContentAlignment="Center"/>
        <Button x:Name="ButtonAdd" Content="确定" HorizontalAlignment="Right" Margin="0,70,10,0" Width="75" Height="25" VerticalAlignment="Top" Click="ButtonAdd_Click"/>
        <TextBlock HorizontalAlignment="Left" Margin="22,15,0,0" TextWrapping="Wrap" Text="请输入密码" VerticalAlignment="Top" />
        <TextBlock HorizontalAlignment="Left" Margin="10,45,0,0" TextWrapping="Wrap" Text="请再输入一次" VerticalAlignment="Top" />

    </Grid>
